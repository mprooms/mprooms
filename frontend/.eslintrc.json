{
    "env": {
        "browser": true,
        "es2020": true,
        "node": true
    },
    "extends": [
        "eslint:recommended",
        "plugin:jest/recommended",
        "plugin:jest/style"
    ],
    "parserOptions": {
        "ecmaVersion": 11,
        "sourceType": "module"
    },
    "rules": {
    },
    "plugins": [
      "jest",
      "svelte3"
    ],
    "overrides": [
      {
        "files": ["*.svelte"],
        "processor": "svelte3/svelte3"
      }
    ]
}
